    {
        "id": "bdbdfd4a323dd33d",
        "type": "function",
        "z": "04fa7c7af98dc038",
        "name": "Format Output Message",
        "func": "var new_msg = { payload: \"\" };\nvar output = { hz_axis_actpos: 0,\n                hz_axis_targetpos: 0,\n                hz_axis_posreached: 0,\n                vert_axis_actpos: 0,\n                vert_axis_targetpos: 0,\n                vert_axis_posreached: 0\n            };\n\n\noutput.hz_axis_actpos = msg.payload[0].value.value;         \noutput.hz_axis_targetpos = msg.payload[1].value.value;      \noutput.hz_axis_posreached =  msg.payload[2].value.value;    \noutput.vert_axis_actpos = msg.payload[3].value.value;       \noutput.vert_axis_targetpos = msg.payload[4].value.value;    \noutput.vert_axis_posreached =  msg.payload[5].value.value;  \n\nnew_msg.payload = output;\n\nreturn new_msg;",
        "outputs": 1,
        "noerr": 0,
        "x": 1030,
        "y": 860,
        "wires": [
