    {
        "id": "9acf9e793864fb54",
        "type": "function",
        "z": "04fa7c7af98dc038",
        "name": "place Item actual Value",
        "func": "// actual value\nmsg.topic = \"HBW_hz_axis_X\"\n\nmsg.top = 300\nmsg.left = 100\nmsg.url = \"url(http://192.168.0.5:1880/images/HBW.png) center/contain no-repeat\";\n\nif (msg.payload.hz_axis_posreached === true)\n    {  \n    msg.state = 1 \n    msg.color = '#5a5'    \n    }\nelse\n    {\n    msg.state = 0\n    msg.color = '#a55'    \n    }\n\n//msg.payload.hz_axis_posreached === true ? msg.state = 1 : msg.state = 0\n\n\nmsg.text = msg.payload.hz_axis_actpos\n\n\nreturn {\n    \"topic\": (msg.topic),\n    \"url\" : (msg.url),\n    \"payload\": {\n        \"icon\":\"label\",\n        \"top\": (msg.top),\n        \"left\":(msg.left),\n        \"color\":(msg.color),\n        \"state\": (msg.state),\n        \"text\": (\"hor. act. Pos.: \" + msg.text)\n\n    }\n\n};\n",
        "outputs": 1,
        "noerr": 0,
        "x": 1310,
        "y": 760,
        "wires": [
