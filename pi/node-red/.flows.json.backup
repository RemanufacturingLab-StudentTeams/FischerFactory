        "id": "1dd275695607ba7e",
        "type": "function",
        "z": "57fb79a2e7a7c26b",
        "name": "Format Output Message",
        "func": "var new_msg = { payload: \"\" };\nvar NFC_out =   {\n                    ts: \"<yyyy-mmddThh:mm:ss.fffZ>\",\n                    workpiece: {id:\"\",type:\"\",state:\"\"},\n                    cmd: \"\",\n                    history:[]\n                }\n\nvar NFC =       {\n                    ts: \"<yyyy-mmddThh:mm:ss.fffZ>\",\n                    cmd: \"\",\n                }\n\n\n    NFC_out.cmd = msg.payload.value[0].value.value;               //[0] = cmd\n    NFC_out.ts =  msg.payload.value[1].value.value;               //[1] = timestamp\n\n    NFC_out.workpiece.id =  msg.payload.value[2].value.value;     //[2] = wp.id\nNFC_out.workpiece.state = msg.payload.value[3].value.value;  //[3] = wp.state\nNFC_out.workpiece.type = msg.payload.value[4].value.value;    //[4] = wp.type\n\n\nif (msg.payload.value[5].value.value === 0)\n        { NFC_out.history = [] }\n    else\n{ NFC_out.history[0] = { \"code\": msg.payload.value[5].value.value, \"ts\": msg.payload.value[6].value.value }; }\n\nif (msg.payload.value[7].value.value === 0)\n        {  }\n    else\n{ NFC_out.history[1] = { \"code\": msg.payload.value[7].value.value, \"ts\": msg.payload.value[8].value.value }; }\n\nif (msg.payload.value[9].value.value === 0)\n        {  }\n    else\n{ NFC_out.history[2] = { \"code\": msg.payload.value[9].value.value, \"ts\": msg.payload.value[10].value.value }; }\n\nif (msg.payload.value[11].value.value === 0)\n        {  }\n    else\n{ NFC_out.history[3] = { \"code\": msg.payload.value[11].value.value, \"ts\": msg.payload.value[12].value.value }; }\n\nif (msg.payload.value[13].value.value === 0)\n        {  }\n    else\n{ NFC_out.history[4] = { \"code\": msg.payload.value[13].value.value, \"ts\": msg.payload.value[14].value.value }; }\n\n\nif (msg.payload.value[15].value.value === 0)\n        {  }\n    else\n{ NFC_out.history[5] = { \"code\": msg.payload.value[15].value.value, \"ts\": msg.payload.value[16].value.value }; }\n\nif (msg.payload.value[17].value.value === 0)\n        {  }\n    else\n{ NFC_out.history[6] = { \"code\": msg.payload.value[17].value.value, \"ts\": msg.payload.value[18].value.value }; }\n\n    if (msg.payload.value[19].value.value === 0)\n        {  }\n    else\n    { NFC_out.history[7] = { \"code\": msg.payload.value[19].value.value, \"ts\": msg.payload.value[20].value.value }; }\n\nif (msg.payload.value[21].value.value === 0)\n        {  }\n    else\n{ NFC_out.history[8] = { \"code\": msg.payload.value[21].value.value, \"ts\": msg.payload.value[22].value.value }; }\n\n\n    if (NFC_out.cmd === \"write\" )\n        { new_msg.payload = JSON.stringify(NFC_out); }\n    else\n        {NFC.cmd = NFC_out.cmd;\n         NFC.ts = NFC_out.ts;\n        new_msg.payload = JSON.stringify(NFC); }\n\nreturn new_msg;",
